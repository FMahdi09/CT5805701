<tr>
	<td>
		<span class="btn btn-danger"
			hx-delete="/users/{{id}}" hx-target="closest tr">
			<i class="bi bi-trash3-fill"></i>
		</span>
		{{id}}
	</td>
	<td> {{first_name}} </td>
	<td> {{last_name}} </td>
	<td> {{username}} </td>
	<td> {{email}} </td>
	<td>
		<span class="btn btn-primary"
			hx-get="/users/{{id}}" hx-target="closest td"
			>
			<i class="bi bi-pencil"></i>
		</span>
		{{username}}
	</td>
</tr>
