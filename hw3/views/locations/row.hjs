<tr class="align-middle">
	<td>
		<span class="btn btn-danger"
			hx-delete="/locations/{{ID}}" hx-target="closest tr"
			>
			<i class="bi bi-trash3-fill"></i>
		</span>
		{{ID}}
	</td>
	<td>{{country}}</td>
	<td>{{state}}</td>
	<td>
		<span class="btn btn-primary"
			hx-get="/locations/{{ID}}" hx-target="closest td"
			>
			<i class="bi bi-pencil"></i>
		</span>
		{{country}}, {{state}}
	</td>
</tr>
