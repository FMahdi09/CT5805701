<tr>
	<td>
		<span class="btn btn-danger"
			hx-delete="/listings/{{id}}?seller={{seller}}" hx-target="closest tr">
			<i class="bi bi-trash3-fill"></i>
		</span>
		{{id}}
	</td>
	<td> {{title}} </td>
	<td> {{price}} </td>
	<td> {{seller}} </td>
	<td>
		<span class="btn btn-primary"
			hx-get="/listings/{{id}}" hx-target="closest td"
			>
			<i class="bi bi-pencil"></i>
		</span>
	</td>
</tr>
